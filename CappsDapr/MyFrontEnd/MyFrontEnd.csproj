    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>a51d5d5e-08b1-40bd-9ef5-b3ae1d3d6d10</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
